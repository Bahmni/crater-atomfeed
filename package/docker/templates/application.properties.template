spring.datasource.url=jdbc:mysql://${CRATER_ATOMFEED_DB_HOST}:${CRATER_ATOMFEED_DB_PORT}/${CRATER_ATOMFEED_DB_NAME}
spring.datasource.username=${CRATER_ATOMFEED_DB_USERNAME}
spring.datasource.password=${CRATER_ATOMFEED_DB_PASSWORD}
spring.jpa.generate-ddl=false
spring.jpa.database-platform=org.hibernate.dialect.MySQLDialect
spring.jpa.show-sql=false
spring.jpa.hibernate.ddl-auto=none
liquibase.change-log=classpath:db/changelog/liquibase.xml
enable.scheduling=true

